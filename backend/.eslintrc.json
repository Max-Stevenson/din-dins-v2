{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "quotes": [2, "double", { "avoidEscape": true }],
        "treatUndefinedAsUnspecified": true 
    }
}
